<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>User Database</title>
    <link rel="stylesheet" href="public/style.css">
  </head>
  <body>
    <section id="user-section">
      {{#users}}
        <div class="users">
          <img src="{{avatar}}" alt="user image" class="center">
          <h2><a href="#">{{name}}</a></h2>
          <h3>{{job}}</h3>
          <h3>{{company}}</h3>
        </div>
      {{/users}}
    </section>
  </body>
</html>
